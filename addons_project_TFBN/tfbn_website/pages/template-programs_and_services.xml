<odoo>
		<template id="website.programs-services" name="Programs and Services" page="True">
    <t t-name="website.programs-services">
      <t t-call="website.layout">
        <div id="wrap" class="oe_structure oe_empty">
          <section class="jumbotron s_big_message oe_img_bg"
          style="background-image: url(&quot;/web/image/website.s_background_image_02&quot;);">

            <div class="container">
              <div class="row">
                <div class="mb0 col-md-10 mt0">
                  <h1>
                    <font class="bg-epsilon text-white"
                    style="font-size: 48px;">Grow your business
                    with the French Community</font>
                  </h1>
                  <p>
                    <font class="text-white" style="">
                      <b>We welcome you, we are here to help and to
                      support you in your growth</b>
                    </font>
                  </p>
                  <a class="btn btn-warning btn-lg o_default_snippet_text"
                  href="/page/website.contactus">Contact us</a>
                </div>
              </div>
            </div>
          </section>
          <section class="s_feature_grid">
            <div class="container">
              <div class="row">
                <div class="col-md-4">
                  <div class="col-md-12 mt16 mb16">
                    <h3 class="mb0">Welcome/Bienvenue</h3>
                    <h5 class="mb16 text-muted">New comers, new
                    businesses</h5>
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-user fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mt0 mb0">Connections</h4>
                    <p>We connect you with the relevant ressources
                    and people &amp;nbsp;</p>
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-file-o fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mb0 mt0">Information
                    <br /></h4>
                    <p>We can provide you the key information to
                    accelerate your entry to the local market.</p>
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="col-md-12 mt16 mb16">
                    <h3 class="mb0">We are here to help</h3>
                    <h5 class="text-muted mb16" align="left">
                    Leverage our network and members to...</h5>
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-binoculars fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mt0 mb0">Hire</h4>
                    <p>Hire talents, connect with other
                    employers</p>
                    <br />
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-compress fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mb0 mt0">Prospect</h4>
                    <p>Find new clients or partners</p>
                    <br />
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-bar-chart-o fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mt0 mb0">Grow</h4>
                    <p>Financing or legal needs, we can help to
                    connect with professionals&amp;nbsp;</p>
                    <br />
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="col-md-12 mt16 mb16">
                    <h3 class="mb0">Entrepreneur Program</h3>
                    <h5 class="mb16 text-muted">Be challenged in
                    your business to grow</h5>
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-slideshare fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mt0 mb0">Discuss with peers</h4>
                    <p>Connect with other entrepreneurs to share
                    your challenges and best practices
                    &amp;nbsp;</p>
                  </div>
                  <div class="col-md-12">
                    <span style="min-width: 45px"
                    class="pull-left mb16 fa fa-dollar fa-2x"
                    data-original-title="" title="" />
                    <h4 class="mb0 mt0">Prepare your financing
                    <br /></h4>
                    <p>Fund raising from business angels, VC, or
                    bank financing, we can challenge your business
                    plan and pitch</p>
                  </div>
                </div>
              </div>
            </div>
          </section>
          <section class="parallax s_parallax oe_img_bg"
          style="background-image: url(&quot;/web/image/website.s_background_image_07&quot;);"
          data-scroll-background-ratio="0.3">
            <div class="oe_structure" />
          </section>
        </div>
      </t>
		</t>
	</template>

		<!-- Adds a menu item -->
		<record id="website.programs-services" model="website.menu">
		  <field name="name">Programs &amp; Services</field>
		  <field name="url">/page/programs-services</field>
		  <field name="parent_id" ref="website.main_menu" />
		  <field name="sequence" type="int">90</field>
		</record>
</odoo>
